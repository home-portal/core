{
  "name": "home-portal",
  "version": "0.1.0-pre",
  "description": "Home Portal Core project",
  "scripts": {
    "dev": "node --inspect node_modules/moleculer/bin/moleculer-runner --repl --hot backend/services/**/*.service.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "dev:module": "node modules/build.js watch",
    "build:module": "node modules/build.js build",
    "build:modules": "node modules/build.js build all",
    "build": "npm run build:frontend",
    "start": "moleculer-runner backend/services/**/*.service.js",
    "install": "npm install && cd frontend && npm install",
    "lint": "eslint backend frontend",
    "deps": "npm-check -u"
  },
  "keywords": [
    "home-portal"
  ],
  "author": "Icebob",
  "devDependencies": {
    "@babel/polyfill": "^7.11.5",
    "@vue/component-compiler-utils": "3.2.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "moleculer-repl": "^0.6.4",
    "npm-check": "^5.9.2",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "prettier": "^2.1.1",
    "sass": "1.26.11",
    "svelte": "^3.29.0",
    "vue": "^2.6.12"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "globby": "^11.0.1",
    "google-auth-library": "^6.1.1",
    "googleapis": "^61.0.0",
    "htmlparser2": "^5.0.0",
    "iconv-lite": "^0.6.2",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "moleculer": "^0.14.10",
    "moleculer-web": "^0.10.0-beta1",
    "nedb": "^1.8.0",
    "node-fetch": "^2.6.1",
    "socket.io": "^2.3.0",
    "vue-hot-reload-api": "2.3.4",
    "xml2js": "^0.4.23"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
